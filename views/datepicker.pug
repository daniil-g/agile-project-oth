extends layout/layout

block content
  main.container(role="main")
    input(data-provide='datepicker')
    .input-group.date(data-provide='datepicker')
      input.form-control(type='text')
      .input-group-addon
        span.glyphicon.glyphicon-th
          input.datepicker(data-date-format='mm/dd/yyyy')
vaadin-date-picker(label='Label')
vaadin-date-picker(placeholder='Placeholder')
vaadin-date-picker(value='1990-01-28')
